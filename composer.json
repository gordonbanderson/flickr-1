{
    "name": "rezzza/flickr",
    "description": "Flickr API Wrapper",
    "keywords": ["flickr"],
    "type": "standalone",
    "homepage": "https://github.com/rezzza/flickr",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/flickr/contributors"
    }],
    "require": {
        "php": ">=5.3.2"
    },
    "suggest": {
        "guzzle/guzzle": "HTTP wrapper",
        "guzzle/http": "Guzzle Adapter for http requests"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6",
        "phpunit/phpunit": "^5"
    },
    "autoload":     {
        "psr-0": {
            "Rezzza\\Flickr": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
